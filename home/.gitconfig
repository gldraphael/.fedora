[user]
  name = Galdin Raphael
  email = gldraphael@users.noreply.github.com
[init]
  defaultBranch = main
[diff]
  # external = difft
  algorithm = histogram
  mnemonicPrefix = true
  renames = true
[tag]
  sort = version:refname
[push]
  autoSetupRemote = true
  followTags = true
[fetch]
  prune = true
  pruneTags = true
  all = true
[commit]
  verbose = true
[rerere]
  enabled = true
  autoupdate = true
[rebase]
  autoSquash = true
  autoStash = true
  updateRefs = true
[pull]
  rebase = true
[core]
  symlinks = true
  eol = native
  autocrlf = false
  editor = nvim
